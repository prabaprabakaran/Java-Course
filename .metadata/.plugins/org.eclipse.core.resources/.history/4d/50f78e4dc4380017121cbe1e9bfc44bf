import org.omg.Messaging.SyncScopeHelper;

class aggre{
	String name;
	int id;
	public aggre(String name,int id){
		this.name=name;
		this.id=id;
	}
	
}
class aggres{
	String add;
	aggre as;
public aggres(String add,aggre as){
	this.add=add;
	this.as=as;
}
	
}
public class example {
public static void main(String[] args) {
	aggres s1=new aggres();
String a=(s1);	
}
}

	System.out.println(str2);
	int number=0;
	for(int i=0;i<str1.length;i++){
	number+=str1[i]-48;
	}
for(char ch='a';ch<='z';ch++){
	for(char ch1='A';ch<='Z';ch1++){
		
	}

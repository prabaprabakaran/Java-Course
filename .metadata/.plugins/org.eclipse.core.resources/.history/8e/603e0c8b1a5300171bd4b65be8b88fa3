package com.sample;

public class throwEx {
	 static void avg() 
	 {
	  try
	  {
	   throw new ArithmeticException("demo");
	  }
	  catch(ArithmeticException e)
	  {
	   System.out.println("Exception caught");
	  } 
	 }

	 public static void main(String args[])
	 {
	  avg(); 
	  System.out.println(5<<3);
}
}
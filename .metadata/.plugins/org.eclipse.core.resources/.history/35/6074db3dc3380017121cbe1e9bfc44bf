import org.omg.Messaging.SyncScopeHelper;

class aggre{
	String name;
	int id;
	public aggre(String name,int id){
		this.name=name;
		this.id=id;
	}
	
}
class aggres{
	aggre as;
	aggre s=new aggre("mathi",10);
public aggres(aggre as) {
	this.as=as;
	
}
	
}
public class example {
public static void main(String[] args) {
	aggres s1=new aggres(s);
	
}
}

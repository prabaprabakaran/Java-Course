package com.sample;

public abstract class Cons {
	int a;
	String b;
Cons(){
	System.out.println("hello");
}
Cons(int a,String b){
	this.a=a;
	this.b=b;
	System.out.println("arg"+ a+" "+b);
}
public static void main(String[] args) {
abs ab=new abs();

}
}
class abs extends Cons{
abs(){
	super(1,"abc");
}
}